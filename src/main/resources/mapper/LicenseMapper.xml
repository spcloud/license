<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.spcloud.license.mapper.LicenseMapper">

    <select id="selectById" resultType="com.spcloud.license.model.License">
        select id, name, org_id from license where id=#{id}
    </select>

</mapper>